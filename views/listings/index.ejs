<% layout("/layouts/boilerplate") %>
<!-- <%= success %> -->
<body>
    <div class=" index row mt-3">
        <div class="col-6 offset-3">
            <h1>All Contacts</h1>
            <% for(listing of allListings) { %>
                <a href="/listings/<%= listing._id %>" style="text-decoration: none;">
                    <div class="card">
                        <div class="card-body">
                        <h5 class="card-title"><i class="fa-solid fa-address-card"></i><%= listing.name %></h5>
                        </div>
                    </div>
                </a>
            <% } %>
        </div>
    </div>
</body>
